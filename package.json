    "drizzle-zod": "^0.5.1",
    "elysia": "latest",
    "pg": "^8.11.3",
    "tsx": "^4.17.0",
    "uploadthing": "^6.13.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@types/pg": "^8.11.0",
    "bun-types": "latest",
    "drizzle-kit": "^0.20.14"
  },
  "module": "src/index.js"
